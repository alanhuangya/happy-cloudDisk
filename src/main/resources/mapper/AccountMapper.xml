<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alan.dao.AccountMapper">


    <select id="selectAccountByEmail" resultType="com.alan.entity.Account">
        SELECT
            *
        FROM user_info
        WHERE email = #{email}
    </select>
</mapper>